{
  "name": "linkedin-scam-detector",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "next": "16.0.0",
      "zod": "^4.1.12",
      "superjson": "^2.2.5",
      "@trpc/server": "^11.5.0",
      "@trpc/client": "^11.5.0",
      "prisma": "^6.19.0",
      "@prisma/client": "^6.19.0",
      "better-auth": "^1.3.34",
      "@polar-sh/sdk": "^0.40.3",
      "dotenv": "^17.2.2",
      "typescript": "^5.9.3",
      "react": "19.2.0",
      "react-dom": "19.2.0",
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "@trpc/tanstack-react-query": "^11.7.1",
      "@trpc/react-query": "^11.7.1",
      "@tanstack/react-query": "^5.90.6",
      "dotenv-cli": "^11.0.0",
      "@temporalio/activity": "^1.13.1",
      "@temporalio/client": "^1.13.1",
      "@temporalio/worker": "^1.13.1",
      "@temporalio/workflow": "^1.13.1"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "clean": "turbo clean",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "format:check": "turbo format",
    "format:write": "prettier --write .",
    "dev:web": "turbo -F @acme/web dev",
    "dev:chrome-extension": "turbo -F @acme/chrome-extension dev",
    "dev:workers": "turbo -F @acme/workers dev",
    "db:push": "turbo -F @acme/db db:push --",
    "db:studio": "turbo -F @acme/db db:studio",
    "db:generate": "turbo -F @acme/db db:generate",
    "db:seed": "turbo -F @acme/db db:seed",
    "schedules:setup": "turbo -F @acme/workers schedules:setup",
    "schedules:list": "turbo -F @acme/workers schedules:list",
    "schedules:pause": "turbo -F @acme/workers schedules:pause --",
    "schedules:unpause": "turbo -F @acme/workers schedules:unpause --",
    "schedules:delete": "turbo -F @acme/workers schedules:delete --",
    "schedules:cleanup": "turbo -F @acme/workers schedules:cleanup",
    "cookies:import": "bun scripts/import-linkedin-cookies.ts",
    "cookies:check": "bun scripts/check-linkedin-auth.ts",
    "schedule:list": "bun scripts/trigger-schedule.ts list",
    "schedule:trigger": "bun scripts/trigger-schedule.ts trigger",
    "workflow:trigger": "bun scripts/trigger-schedule.ts workflow",
    "process:discovered-jobs": "bun scripts/process-discovered-jobs.ts",
    "docker:up": "docker compose -f ./docker-compose.yml up -d",
    "docker:down": "docker compose -f ./docker-compose.yml down",
    "docker:stop": "docker compose -f ./docker-compose.yml stop",
    "docker:start": "docker compose -f ./docker-compose.yml start",
    "docker:restart": "docker compose -f ./docker-compose.yml restart",
    "docker:clean": "docker compose -f ./docker-compose.yml down -v",
    "docker:logs": "docker compose -f ./docker-compose.yml logs",
    "docker:exec": "docker compose -f ./docker-compose.yml exec",
    "docker:exec:bash": "docker compose -f ./docker-compose.yml exec redis bash"
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "@types/node": "^22.13.11",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.2.23",
  "patchedDependencies": {
    "jiti@2.6.1": "patches/jiti@2.6.1.patch",
    "@tailwindcss/oxide@4.1.16": "patches/@tailwindcss%2Foxide@4.1.16.patch"
  }
}
