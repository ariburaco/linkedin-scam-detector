{
  "name": "@acme/api",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./routers": {
      "types": "./src/routers/index.ts",
      "default": "./src/routers/index.ts"
    },
    "./context": {
      "types": "./src/context.ts",
      "default": "./src/context.ts"
    },
    "./services/embedding.service": "./src/services/embedding.service.ts",
    "./services/ai.service": "./src/services/ai.service.ts",
    "./services/batch-embedding.service": "./src/services/batch-embedding.service.ts",
    "./services/temporal.service": "./src/services/temporal.service.ts",
    "./services/job.service": "./src/services/job.service.ts",
    "./services/job-analysis.service": "./src/services/job-analysis.service.ts",
    "./services/job-extraction.service": "./src/services/job-extraction.service.ts",
    "./services/job-search.service": "./src/services/job-search.service.ts",
    "./services/feedback.service": "./src/services/feedback.service.ts",
    "./services/discovered-job.service": "./src/services/discovered-job.service.ts",
    "./services/feature-flags.service": "./src/services/feature-flags.service.ts",
    "./schemas/job-extraction": "./src/schemas/job-extraction.ts",
    "./schemas/feature-flags": "./src/schemas/feature-flags.ts"
  },
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:write": "prettier --write . --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@acme/config-eslint-prettier": "workspace:*"
