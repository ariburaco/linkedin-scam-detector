  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/db": "workspace:*",
    "@ai-sdk/google": "^2.0.28",
    "@google/generative-ai": "^0.24.1",
    "@temporalio/client": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "ai": "^5.0.87",
    "dotenv": "catalog:",
    "next": "catalog:",
    "superjson": "catalog:",
    "trpc-redis-cache": "^0.3.0",
    "zod": "catalog:"
  }
